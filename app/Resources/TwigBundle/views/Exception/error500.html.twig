{% extends "TwigBundle::layout.html.twig" %}


{% block stades_title_error %}
    Erreur {{ status_text }}
{% endblock %}

{% block stades_sub_title_error %}
    Erreur {{ status_code }}
{% endblock %}

{% block  stades_view_error %}
